	keywords = {Anomaly detection, Arrays, Dimensionality reduction, gamma-ray detection, gamma-ray spectral analysis, Gamma-rays, Hyperspectral imaging, Mathematical model, non-negative matrix factorization (NMF), Principal component analysis, radiation source search},
	pages = {827--837},
	file = {Eingereichte Version:/home/raedlev/Zotero/storage/6C434LQY/Bilton et al. - 2019 - Non-negative Matrix Factorization of Gamma-Ray Spectra for Background Modeling, Detection, and Sourc.pdf:application/pdf;IEEE Xplore Abstract Record:/home/raedlev/Zotero/storage/JNR8HE6J/8673769.html:text/html},

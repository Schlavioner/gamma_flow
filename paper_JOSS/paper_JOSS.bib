	keywords = {Principal component analysis, Biomedical signal processing, Collective factorization, Data mining, Electroencephalography, Feature extraction, Image analysis, Machine learning, Matrix decomposition, nonnegative matrix factorization, semi-supervised learning, Semisupervised learning, Spectrogram},
	pages = {4--7},
	file = {IEEE Xplore Abstract Record:/home/raedlev/Zotero/storage/QSK3TRI4/5161276.html:text/html},
}

